config/icon="res://YarnSpinnerLogo.png"
config/features=PoolStringArray( "4.0", "C#" )

[display]

window/size/resizable=false
window/size/viewport_width=1600
window/size/viewport_height=900

[dotnet]

project/assembly_name="YarnSpinnerGodot"

[editor_plugins]

enabled=PoolStringArray( "res://addons/YarnSpinnerGodot/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[mono]

project/assembly_name="YarnSpinner-Godot"

[physics]

common/enable_pause_aware_picking=true
